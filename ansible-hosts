[all:vars]
ansible_user=vagrant
ansible_ssh_host=localhost
ansible_ssh_common_args='-o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no'

[loadbalancer]
lb01 ansible_ssh_port=2200

[webserver]
app01 ansible_ssh_port=2201
app02 ansible_ssh_port=2202

[database]
db01 ansible_ssh_port=2203

[control]
control01 ansible_ssh_port=2222
